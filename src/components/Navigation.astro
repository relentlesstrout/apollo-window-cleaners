<header class="bg-white sticky top-0 z-50">
    <nav class="relative mx-auto max-w-7xl flex items-center p-6 lg:px-8">
        <!-- Logo on the left -->
        <div class="flex items-center">
            <a href="#" class="-m-1.5 p-1.5">
                <span class="sr-only">Your Company</span>
                <img src="/Apollo-Logo-2.jpg" class="w-12 h-12" alt="Company Logo">
            </a>
        </div>

        <!-- Centered navigation links -->
        <div class="absolute left-1/2 transform -translate-x-1/2 flex gap-x-6 lg:gap-x-12">
            <a href="/" class="text-m font-semibold text-blue-950 hover:text-indigo-600">Home</a>
            <a href="/Quote" class="text-m font-semibold text-blue-950 hover:text-indigo-600">Quote</a>
            <a href="/About" class="text-m font-semibold text-blue-950 hover:text-indigo-600">About</a>
        </div>

        <!-- Right side (login) -->
        <div class="ml-auto lg:flex">
            <a href="#" class="text-sm/6 font-semibold text-white bg-blue-950 px-4 py-2 rounded hover:bg-gray-800">
                Log in <span aria-hidden="true">&rarr;</span>
            </a>
        </div>
    </nav>
</header>

